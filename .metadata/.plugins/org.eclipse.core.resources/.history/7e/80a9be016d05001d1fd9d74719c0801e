package Video1;

public class Proje1 {

	public static void main(String[] args) {

		int sayi1 = 10;
		int sayi2 = 20;

		sayi1 = sayi2;

		sayi2 = 200;

		System.out.println("sayi1 : " + sayi1);
		System.out.println("sayi2 : " + sayi2);

		int dizi1[] = { 10, 20, 30, 40, 50 };
		int dizi2[] = { 60, 70, 80, 90, 100 };

		dizi1 = dizi2;

		dizi2[1] = 1000;

		System.out.println("dizi 1 : " + dizi1[1]);
		System.out.println("dizi 2 : " + dizi2[1]);

	}

}
